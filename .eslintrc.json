{
    "plugins": [
        "react"
    ],
    "env": {
        "browser": true,
        "es2022": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:react/jsx-runtime",
        "eslint-config-prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx":true
        }
    },
    "rules": {
        "multiline-ternary":["warn","always"],
        "linebreak-style":["error","unix"],
        "indent":["error", 2],
        "no-var":"error",
        "no-new-func":"error",
        "no-alert":"warn",
        "no-console":"warn",
        "eqeqeq":"error",
        "no-duplicate-imports": ["error", {"includeExports": true}],
        "no-duplicate-case": ["error"]
    }
}